            <TextField fx:id="name" layoutX="40.0" layoutY="34.0" promptText="Name" />
            <TextField fx:id="number" layoutX="40.0" layoutY="59.0" promptText="Number"/>
            <Button layoutX="132.0" layoutY="270.0" mnemonicParsing="false" text="UpdateName" onAction="#updatename"/>
